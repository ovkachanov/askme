<div class="content-wrapper buffer-both">
  <div class="buffer-both text-center">
    <h1>Все вопросы по хештегу </h1>
    <h2 class="hashtag-title">#<%= @hashtag.name %></h2>
  </div>
</div>

<div class="user-questions buffer-both">
  <div class="questions-list">
    <% @questions.uniq.each do |question| %>
      <%= render partial: 'questions/question', object: question %>
    <% end %>
  </div>
</div>
